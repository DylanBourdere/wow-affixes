{% macro display(affixes) %}
    {% if affixes is same as("Unknown") %}
        {% trans %}Not defined yet{% endtrans %}
    {% else %}
        {% for affix in affixes %}
            <a href="https://{% trans %}www{% endtrans %}.wowhead.com/affix={{ affixes[i] }}/" class="affixes affixes-{{ affixes[i] }}"></a><br/>
        {% endfor %}
    {% endif %}
{% endmacro %}